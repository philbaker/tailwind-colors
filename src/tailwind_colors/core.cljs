(defn key-from-value [value coll]
  (first (filter (comp #{value} coll) (keys coll))))

(defn hex-to-class 
  "Get class name based on hex value"
  [hex-code]
  (name (key-from-value hex-code colors-hex)))

(defn rgb-to-class 
  "Get class name based on rgb value"
  [rgb-code]
  (name (key-from-value rgb-code colors-rgb)))

(defn class-to-hex 
  "Get hex code based on class name"
  [class-name]
  ((keyword class-name) colors-hex))

(defn class-to-rgb 
  "Get rgb code based on class name"
  [class-name]
  ((keyword class-name) colors-rgb))

; Command line arguments
(def cmd-line-args (js->clj (.slice js/process.argv 2)))
(def command (first cmd-line-args))
(def color (second cmd-line-args))

(when (or (nil? command) (nil? color))
  (println "two arguments required")
  (js/process.exit 1))

(when (or (= command "hc") (= command "hex-to-class"))
  (println (hex-to-class color)))

(when (or (= command "rc") (= command "rgb-to-class"))
  (println (rgb-to-class color)))

(when (or (= command "ch") (= command "class-to-hex"))
  (println (class-to-hex color)))

(when (or (= command "cr") (= command "class-to-rgb"))
  (println (class-to-rgb color)))

(comment
  ; Generate JSON
  (.stringify js/JSON (clj->js colors-hex))
  (.stringify js/JSON (clj->js colors-rgb))
  )
